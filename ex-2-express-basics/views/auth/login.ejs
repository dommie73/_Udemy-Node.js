<%- include('../partials/head'); -%>

<form
	autocomplete="off"
	action="/login"
	method="POST"
>
	<h1>
		Login
	</h1>
	<div>
		<label for="email">Email</label>
		<input
			id="email"
			name="email"
			type="text"
		/>
	</div>
	<div>
		<label for="password">Password</label>
		<input
			id="password"
			name="password"
			type="password"
		/>
	</div>
	<button class="button" type="submit">
		Login
	</button>
</form>

<%- include('../partials/end'); -%>
